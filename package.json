  "main": "index.js",
  "scripts": {
    "build": "npx tsc && tsc-alias",
    "dev": "nodemon -e ts,js --exec ts-node -r tsconfig-paths/register ./src/server.ts",
    "start": "node ./dist/server.js",
    "migration:create": "npx typeorm-ts-node-commonjs migration:create",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate -d ./src/config/data-source.ts",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d ./src/config/data-source.ts",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d ./src/config/data-source.ts",
    "seed": "ts-node -r tsconfig-paths/register ./src/seed/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@joi/date": "^2.1.0",
    "bcrypt": "^5.1.0",
    "cloudinary": "^1.37.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "mssql": "^9.1.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.4.0",
    "prettier": "^2.8.8",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
